#include <stido.h>
char x[20]={0}; //使用者输入的密码
char y[20]={"123456"}; //正确的密码

int main()
{
	char c1;
	char c2;
	printf("请输入六位数密码\n");
	scanf("%s",x);
	while((c2=getchar())!=10);
	printf("请确认(Y/N)\n");
	if((c1=getchar())='Y');
	{
		if(x[0]==y[0]) printf("密码正确\n");
		else printf("密码错误\n");
	}
	return 0;

}
